      <!--suppressed optional element initial of type ReportedTimeType-->
      <!--suppressed optional element target of type ReportedTimeType-->
   </PositionMultiTimeType>

<!--Type RunwayPositionType-->
   <RunwayPositionType runwayName="string"/>

<!--Type StandPositionType-->
   <StandPositionType standName="string" terminalName="string"/>

<!--Type UnlistedAerodromeReferenceType-->
   <UnlistedAerodromeReferenceType name="string">
